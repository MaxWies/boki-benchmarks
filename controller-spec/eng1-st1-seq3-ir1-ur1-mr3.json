{
    "log_engine_replicas": 1,
    "log_storage_replicas": 1,
    "log_sequencers_replicas": 3,
    "log_controller": {
        "image": "zjia/boki:sosp-ae",
        "entrypoint" : [
            "/slog/controller", 
            "--zookeeper_host=zookeeper:2181", 
            "--metalog_replicas=3", 
            "--userlog_replicas=1", 
            "--index_replicas=1"
