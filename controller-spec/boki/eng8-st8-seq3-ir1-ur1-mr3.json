{
    "slog_engine_replicas": 8,
    "slog_storage_replicas": 8,
    "slog_sequencers_replicas": 3,
    "slog_controller": {
        "image": "maxwie/boki:thesis-sub",
        "entrypoint" : [
            "/slog/controller", 
            "--zookeeper_host=zookeeper:2181", 
            "--metalog_replicas=3", 
            "--userlog_replicas=1", 
            "--index_replicas=1"
        ],
        "depends_on": [
            "zookeeper-setup"
        ]
    }
}