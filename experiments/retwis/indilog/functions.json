    "functions": {
        "RetwisInit": {
            "service_name": "retwis-init",
            "docker": {
                "image": "maxwie/boki-retwisbench:latest",
                "entrypoint" : ["/tmp/boki/run_launcher", "/retwisbench-bin/main"],
                "volumes": ["/mnt/inmem/boki:/tmp/boki"],
                "environment": ["FAAS_GO_MAX_PROC_FACTOR=4", "GOGC=200"],
                "depends_on": ["boki-engine"]
            }
        },
